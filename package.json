{
  "name": "yeshie",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev:server": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts",
    "dev:client": "cd client && pnpm run dev",
    "dev": "concurrently \"pnpm run dev:monitor\" \"pnpm run dev:server\" \"pnpm run dev:client\" \"pnpm run dev:extension\" \"pnpm run dev:messages\"",
    "build:client": "cd client && pnpm run build",
    "build:server": "tsc",
    "build:repl": "pnpm run build:server && pnpm run build:client",
    "dev:listener": "nodemon --watch src/listener.py --watch src/rewind.py --exec python src/listener.py",
    "dev:monitor": "nodemon --watch src/monitor.py --watch src/rewind.py --watch src/calibrate.py --watch src/llmdebug.py --watch src/customprint.py --watch src/llmserver.py --watch src/vectorstore.py --watch src/codeStore.py --watch src/gitignore.py --watch src/embedding_model.py --watch src/monitor_env_handler.py --exec python src/monitor.py",
    "dev:testkb": "nodemon --watch src/testkb.py --exec python src/testkb.py",
    "dev:winmonitor": "nodemon --watch src/winmonitor.py --exec python src/winmonitor.py",
    "dev:extension": "cd extension && pnpm run dev",
    "dev:messages": "nodemon --watch scripts/messages.sh --ignore messages.txt --exec ./scripts/messages.sh -watch scripts/messages.sh",
    "devx": "concurrently \"pnpm run dev:winmonitor\" \"pnpm run dev:extension\"",
    "dev1": "concurrently $(pnpm run --silent | grep '^dev:' | awk '{print $1}')",
    "dev:scrape": "nodemon --watch src/scrape.py --exec python src/scrape.py",
    "test:client": "cd client && pnpm run test",
    "test:python": "python src/run_tests.py",
    "test": "pnpm run test:client && pnpm run test:python"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@babel/parser": "^7.x",
    "@babel/traverse": "^7.x",
    "@codemirror/collab": "^6.1.1",
    "@codemirror/state": "^6.4.1",
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.13.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-tools": "^13.20.2",
    "lucide-react": "^0.441.0",
    "react-icons": "^5.3.0",
    "socket.io": "^4.7.5",
    "typescript": "^4.x",
    "uuid": "^11.0.5",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/concurrently": "^6.4.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "@types/socket.io": "^3.0.1",
    "concurrently": "^8.2.2",
    "http-proxy-middleware": "^3.0.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.9"
  },
  "workspaces": [
    "client",
    "extension",
    "shared"
  ]
}
